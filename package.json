{
  "name": "@valentynb/gs1-parser",
  "version": "1.0.0",
  "description": "A typescript gs1-barcode parser. Based on code from https://github.com/flashcloud/gs1-parser",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts --dts --format esm,cjs --clean --minify --keep-names",
    "test": "vitest"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "gs1",
    "barcode",
    "parsing",
    "logistics"
  ],
  "author": "Valentyn Blaha",
  "license": "MIT",
  "homepage": "https://github.com/valentynblaha/gs1-parser#readme",
  "bugs": {
    "url": "https://github.com/valentynblaha/gs1-parser/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.46.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  }
}
